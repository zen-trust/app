{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true,
      "importAssertions": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "target": "esnext",
    "loose": true,
    "preserveAllComments": false,
    "keepClassNames": false
  },
  "sourceMaps": true,
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": false,
    "lazy": false,
    "noInterop": true
  },
  "isModule": true
}

